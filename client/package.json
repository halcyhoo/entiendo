{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar ./sass -o ./src --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar ./sass -o ./src --watch --recursive",
    "start-js": "PORT=3001 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3000",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.0.0",
    "dom-confetti": "0.0.10",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-dom-confetti": "0.0.8",
    "react-modal": "^3.1.12",
    "react-portal": "^4.1.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "react-toastify": "^3.3.2",
    "responsivevoice": "^0.4.1"
  },
  "devDependencies": {}
}
